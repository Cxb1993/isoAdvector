FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
	location		"system";
    object          controlDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     $solver;

startFrom       latestTime;

startTime       0.0;

stopAt          endTime;

endTime         $T;

writeControl    timeStep;

writeInterval   $wInt;

deltaT          $dt;

purgeWrite      0;

writeFormat     ascii;

writePrecision  14;

writeCompression uncompressed;

timeFormat      general;

timePrecision   14;

graphFormat     raw;

runTimeModifiable no;

adjustTimeStep  yes;

maxCo           1e6;
maxAlphaCo      $CoNum;

maxDeltaT       1e6;

libs ( "libVOFInterpolationSchemes.so" );

// ************************************************************************* //
