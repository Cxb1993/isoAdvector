FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    location        "system";
    object          setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha1 0
);

regions
(
    boxToCell
    {
        box (-1e6 -1e6 -1e6) (1e6 1 1e6);
        fieldValues
        (
            volScalarFieldValue alpha1 1
        );
    }
);


