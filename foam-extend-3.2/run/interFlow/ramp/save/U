FoamFile
{
    version  2.0;
    format   ascii;
    class    volVectorField;
    location "0";
    object   U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (6 0 0);

boundaryField
{
    inletWater
    {
        type            fixedValue;
        value           uniform (6 0 0);
    }
    inletAir
    {
        type            fixedValue;
        value           uniform (6 0 0);
    }
    bottom
    {
        type            slip;
    }
    outlet
    {
        type            zeroGradient;
    }
    atmosphere
    {
        type            slip;
    }
    defaultFaces
    {
        type            empty;
    }
}


// ************************************************************************* //
